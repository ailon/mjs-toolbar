    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "prettier": "^2.1.1",
    "rollup": "^2.26.5",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dev": "^1.1.2",
    "rollup-plugin-dts": "^1.4.12",
